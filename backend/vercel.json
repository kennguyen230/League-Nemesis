{
  "version": 2,
  "builds": [{ "src": "./index.ts", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ],
  "functions": {
    "index.ts": {
      "maxDuration": 60
    }
  }
}
