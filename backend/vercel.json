{
  "version": 2,
  "functions": {
    "./index.ts": {
      "maxDuration": 60
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.ts",
      "methods": ["GET", "POST", "PUT", "DELETE", "PATCH", "OPTIONS", "HEAD"],
      "headers": {
        "Access-Control-Allow-Origin": "*"
      }
    }
  ]
}
